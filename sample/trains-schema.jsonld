{
	"@context": "http://www.w3.org/ns/csvw",
	"url": "12800-sample.csv",
	"dialect": {
		"encoding": "utf-8"
	},
	"tableSchema": {
		"primaryKey": [ "barcode_number", "image" ],
		"aboutUrl": "/rest/records/{barcode_pairtree}/images/{image}",
		"columns": [
			{
				"csvw:note": "This column is just a string valued property of a barcode, which is a CIDOC E42 Identifier generated by the virtual column 'barcode', below",
				"name": "barcode_number",
				"required": true,
				"aboutUrl": "/rest/records/{barcode_pairtree}#barcode",
				"propertyUrl": "rdf:value"
			},
			{
				"csvw:note": [
					"This column is the 'pairtree' version of a barcode for use in the path of a Fedora URI. ",
					"Output is suppressed because it's only used as an 'aboutUrl', i.e. to specify the subject of triples whose objects are the values of in the other CSV columns."
				],
				"name": "barcode_pairtree",
				"required": true,
				"suppressOutput": true
			},
			{
				"name": "image",
				"required": true,
				"datatype": { "base": "integer" },
				"suppressOutput": false,
				"propertyUrl": "rdfs:label"
			},
			{
				"name": "series",
				"csvw:note": [
					"Expressed as a locally-defined property",
					"TODO: check; is the 'series' a logical grouping of consignments?"
				],
				"propertyUrl": "#series",
				"suppressOutput": false
			},
			{
				"name": "consignment",
				"csvw:note": [
					"Expressed as a locally-defined property",
					"TODO: check; is the 'consignment' a logical grouping of units?"
				],
				"propertyUrl": "#consignment",
				"suppressOutput": false
			},
			{
				"name": "unit",
				"csvw:note": [
					"Expressed as a locally-defined property",
					"TODO: check; is the 'unit' a logical grouping of images?"
				],
				"propertyUrl": "#unit",
				"suppressOutput": false
			},
			{
				"name": "title_text",
				"required": true,
				"aboutUrl": "/rest/records/{barcode_pairtree}/images/{image}#title",
				"propertyUrl": "rdf:value"
			},
			{
				"name": "desc2",
				"csvw:note": "Some kind of identifier?",
				"suppressOutput": true
			},
			{
				"name": "sequence_number",
				"csvw:note": "Within what context does the sequence number apply? It's not unique. Meaning unclear, so expressed as a locally-defined property",
				"propertyUrl": "#sequence_number",
				"suppressOutput": false
			},
			{
				"name": "photographic_media_type",
				"csvw:note": "e.g. 'GLS' = glass plate, 'B/W' = acetate film (black and white)",
				"propertyUrl": "http://purl.org/dc/terms/medium",
				"valueUrl": "#medium-{photographic_media_type}",
				"suppressOutput": false
			},
			{
				"name": "desc4",
				"csvw:note": "some kind of descriptor, with the following column as a refinement?",
				"suppressOutput": true
			},
			{
				"name": "desc5",
				"suppressOutput": true
			},
			{
				"csvw:note": "What kind of identifier?",
				"name": "identifier",
				"suppressOutput": true
			},
			{
				"name": "scanning_date",
				"csvw:note": "TODO use a better date format; CSV has '1-Aug-01'; it should be some format compatible with an xml schema date type e.g. '2001-07-01' or similar",
				"suppressOutput": true
			},
			{
				"csvw:note": "TODO",
				"name": "resolution",
				"suppressOutput": true
			},
			{
				"csvw:note": "TODO",
				"name": "pixel_dimensions",
				"suppressOutput": true
			},
			{
				"csvw:note": "TODO",
				"name": "physical_cdrom",
				"suppressOutput": true
			},
			{
				"name": "barcode",
				"propertyUrl": "http://www.cidoc-crm.org/cidoc-crm/E42_Identifier",
				"aboutUrl": "/rest/records/{barcode_pairtree}",
				"valueUrl": "/rest/records/{barcode_pairtree}#barcode",
				"virtual": true
			},
			{
				"name": "title",
				"propertyUrl": "http://www.cidoc-crm.org/cidoc-crm/E35_Title",
				"valueUrl": "/rest/records/{barcode_pairtree}/images/{image}#title",
				"virtual": true
			},
			{
				"csvw:note": "Assert a match between the 'GLS' code and The Getty's 'Glass Plate Negative'.",
				"name": "glass_plate",
				"aboutUrl": "#medium-GLS",
				"propertyUrl": "http://www.w3.org/2004/02/skos/core#exactMatch",
				"valueUrl": "http://vocab.getty.edu/aat/300393160",
				"virtual": true
			},
			{
				"name": "black_and_white",
				"csvw:note": [
					"Assert a match between the 'B/W' code and The Getty's 'Acetate Film'.",
					"TODO: check if this is in fact a close match or an exact match for AAT's 'acetate film'?"
				],
				"aboutUrl": "#medium-B/W",
				"propertyUrl": "http://www.w3.org/2004/02/skos/core#closeMatch",
				"valueUrl": "http://vocab.getty.edu/aat/300255863",
				"virtual": true
			},
			{
				"name": "glass_black_and_white_narrower_than_black_and_white",
				"csvw:note": "Assert that the GLSB/W code is a narrower term of B/W",
				"aboutUrl": "#medium-GLSB/W",
				"propertyUrl": "http://www.w3.org/2004/02/skos/core#narrower",
				"valueUrl": "#medium-B/W",
				"virtual": true
			},
			{
				"name": "glass_black_and_white_narrower_than_glass",
				"csvw:note": "Assert that the GLSB/W code is a narrower term of GLS",
				"aboutUrl": "#medium-GLSB/W",
				"propertyUrl": "http://www.w3.org/2004/02/skos/core#narrower",
				"valueUrl": "#medium-GLS",
				"virtual": true
			}
		]
	}
}